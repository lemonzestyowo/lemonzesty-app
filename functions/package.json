{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "genkit/lib/configure": ["node_modules/genkit/lib/configure.d.ts"],
      "genkit/lib/flow": ["node_modules/genkit/lib/flow.d.ts"],
      "genkit/lib/generate": ["node_modules/genkit/lib/generate.d.ts"],
      "genkit/lib/context": ["node_modules/genkit/lib/context.d.ts"],
      "genkit/lib/genkit": ["node_modules/genkit/lib/genkit.d.ts"],
      "genkit/lib/auth/auth": ["node_modules/genkit/lib/auth/auth.d.ts"],
      "genkit/lib/ai/model": ["node_modules/genkit/lib/ai/model.d.ts"],
      "genkit/firebase/lib/index": ["node_modules/genkit/firebase/lib/index.d.ts"],
      "genkit/vertexai/lib/index": ["node_modules/genkit/vertexai/lib/index.d.ts"]
    }
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
